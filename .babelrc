{
  "presets": ["es2015", "stage-0", "react"],
  "env": {
    "development": {
      "plugins": ["react-hot-loader/babel"]
    },
    "production": {
        "presets": ["react-optimize"],
        "plugins": [
            "transform-remove-console",
            "transform-remove-debugger",
            "dev-expression"
        ]
    }
  }
}
